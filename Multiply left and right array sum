//  Link for the problem:  https://practice.geeksforgeeks.org/problems/multiply-left-and-right-array-sum1555/1/


int multiply(int arr[], int n)
{
    // Complete the function
    int arrl=0,arrr=0;
    
    for(int i =0; i<n/2; i++)
        arrl+=arr[i];
    for(int i =n/2; i<n; i++)
        arrr+=arr[i];
    return arrr*arrl;
}
